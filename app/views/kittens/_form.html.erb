<%= form_with model: @kitten do |f| %>
<p>
  <%= f.label :name %> <br>
  <%= f.text_field :name %>
</p>

<p>
  <%= f.label :age %> <br>
  <%= f.text_field :age  %> <br>
</p>

<p>
  <%= f.label :cuteness %> <br>
  <%= f.text_field :cuteness %>
</p>

<p>
  <%= f.label :softness %> <br>
  <%= f.text_field :softness %>
</p>

<p><%= f.submit "save" %> </p>

<% end %>